{
  "name": "sizemometer",
  "description": "A library for monitoring your bundle sizes",
  "version": "0.7.4",
  "bin": {
    "sizemometer": "bin/main"
  },
  "scripts": {
    "start": "tsc --watch",
    "build": "node scripts/copy-static-files",
    "test": "ENV=test jest",
    "lint": "eslint 'src/**/*.js' 'website/**/*.js' --ignore-pattern '*.test.js'",
    "lint:fix": "eslint 'src/**/*.js' 'website/**/*.js' --fix --ignore-pattern '*.test.js'",
    "prepublish": "npm run build",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chardos/sizemometer.git"
  },
  "main": "src/api/index.js",
  "dependencies": {
    "babel-loader": "7.1.5",
    "babel-polyfill": "6.26.0",
    "babel-preset-react": "6.24.1",
    "es6-promisify": "6.0.1",
    "filesize": "3.6.1",
    "find-up": "3.0.0",
    "fs-extra": "7.0.1",
    "get-git-data": "1.3.1",
    "glob": "7.1.3",
    "gzip-size": "5.0.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.debounce": "4.0.8",
    "lodash.get": "4.4.2",
    "make-dir": "1.3.0",
    "minimist": "1.2.0",
    "node-cmd": "3.0.0",
    "opn": "5.4.0",
    "ramda": "0.26.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "rimraf": "2.6.2",
    "sizemometer-report": "^0.1.0",
    "styled-components": "4.1.3",
    "styled-normalize": "8.0.4",
    "webpack": "4.28.3",
    "webpack-cli": "3.1.2"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.0",
    "eslint-plugin-react": "7.12.4",
    "jest": "23.6.0",
    "regenerator-runtime": "0.13.1",
    "semantic-release": "^17.2.3"
  }
}
